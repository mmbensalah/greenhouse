<h1>Edit<%= @plant.name %></h1>

<%= form_for [:admin, @plant] do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :GreenHouse %>
  <%= f.collection_select(:green_house_id, @green_houses, :id, :name) %>

  <%= f.label :variety %>
  <%= f.text_field :variety %>

  <%= f.label :sell_quantity %>
  <%= f.text_field :sell_quantity %>

  <%= f.label :stock_plants %>
  <%= f.text_field :stock_plants %>

  <%= f.label :zone %>
  <%= f.text_field :zone %>

  <%= f.label :shipping %>
  <%= f.text_field :shipping %>

  <%= f.label :image %>
  <%= f.text_field :image %>

<%= f.submit "Submit changes" %>
<% end %>
